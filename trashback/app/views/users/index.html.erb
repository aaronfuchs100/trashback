<div id="users">

    <% @users.each do |user| %>
        <div class="CSSTableGenerator" >
          	<table >

          		<tr> 
					<td>
						Name:
					</td>
					<td >
						Number:
					</td>
					<td>
						Email:
					</td>
				</tr>

				<tr>
					<td >
						<%= link_to user.name, user %>
					</td>

					<td >
						<%= user.phone %>
					</td>

					<td >
						<%= user.email %>
					</td>
				</tr>
			</table >
		</div>
            
          <% end %>

</div>



